<h1>Login</h1>
<%= form_tag '/login' do %>
  <%= label_tag "Email" %>
  <%= email_field_tag :email %>
  <br/>
  <%= label_tag "Password" %>
  <%= password_field_tag :password %>

  <%= link_to "Forget passwrord", "/reset_password" %>


  <br/>
  <%= submit_tag "Login now!!!!" %>
  <%= @login_message %>
<% end %>
